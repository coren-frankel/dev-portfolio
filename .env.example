# Resend API Key (get from https://resend.com/api-keys)
RESEND_API_KEY=your_resend_api_key_here

# Cloudflare Turnstile Secret Key (get from https://dash.cloudflare.com/turnstile)
TURNSTILE_SECRET_KEY=your_turnstile_secret_key_here

# Cloudflare Turnstile Site Key (get from https://dash.cloudflare.com/turnstile)
# This will be passed to the client-side contact form via the loader
TURNSTILE_SITE_KEY=your_turnstile_site_key_here
