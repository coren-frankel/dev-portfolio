{
  "name": "dev-portfolio",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "lint": "oxlint",
    "prepare": "husky || true",
    "format": "prettier --write .",
    "typecheck": "tsc-files --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@react-spring/parallax": "^10.0.1",
    "@react-spring/web": "^10.0.1",
    "@react-three/fiber": "^9.3.0",
    "antd": "^5.27.1",
    "easing-coordinates": "^2.0.2",
    "emoji-regex": "^10.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.2",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/three": "^0.179.0",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "oxlint": "^1.12.0",
    "prettier": "^3.6.2",
    "tsc-files": "^1.1.4",
    "typescript": "5.9.2",
    "vite": "^7.1.3"
  },
  "lint-staged": {
    "*.{ts,tsx,json,css}": [
      "prettier --write"
    ],
    "*.{js,ts,jsx,tsx,mjs,cjs,mts,cts}": [
      "pnpm lint"
    ],
    "*.{ts,tsx}": [
      "pnpm typecheck"
    ]
  }
}
