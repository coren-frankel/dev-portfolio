    "@react-router/serve": "^7.8.2",
    "@react-spring/parallax": "^10.0.1",
    "@react-spring/web": "^10.0.1",
    "@react-three/fiber": "^9.3.0",
    "antd": "^5.27.1",
    "easing-coordinates": "^2.0.2",
    "emoji-regex": "^10.4.0",
    "isbot": "^5.1.30",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2",
    "resend": "^6.0.1",
    "three": "^0.179.1"
