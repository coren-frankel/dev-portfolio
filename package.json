    "oxlint": "^1.13.0",
    "prettier": "^3.6.2",
    "tsc-files": "^1.1.4",
    "typescript": "5.9.2",
    "vite": "^7.1.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.33.1"
  },
  "lint-staged": {
    "*.{ts,tsx,json,css}": [
      "prettier --write"
